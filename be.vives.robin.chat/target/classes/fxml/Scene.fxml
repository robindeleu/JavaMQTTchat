<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="400.0" styleClass="AnchorPane" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.vives.robin.chat.FXMLController">
   <children>
      <VBox layoutX="-1.0" prefHeight="20.0" prefWidth="400.0">
         <children>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Please login" textAlignment="CENTER" wrappingWidth="400.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Times New Roman Bold Italic" size="26.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox layoutX="99.0" layoutY="100.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="nickname" prefHeight="26.0" prefWidth="220.0" styleClass="TextField" stylesheets="@../styles/Styles.css">
               <opaqueInsets>
                  <Insets top="100.0" />
               </opaqueInsets>
            </TextField>
         </children>
      </VBox>
      <VBox layoutX="323.0" layoutY="100.0" prefHeight="100.0" prefWidth="75.0">
         <children>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#handlelogin" styleClass="Button" stylesheets="@../styles/Styles.css" text="Login" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
